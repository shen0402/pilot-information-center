      {% if section.settings.tab4_header != blank %}
        <a data-w-tab="Tab 4" class="btn-wrapper-types w-inline-block w-tab-link">
          <div>View »</div>
        </a>
      {% endif %}
    </div>
    <div class="w-tab-content">
      {% assign tab1 = section.blocks | where: 'type', 'tab1' %}
      {% assign tab2 = section.blocks | where: 'type', 'tab2' %}
      {% assign tab3 = section.blocks | where: 'type', 'tab3' %}
      {% assign tab4 = section.blocks | where: 'type', 'tab4' %}
      {% if tab1.size > 0 %}
        <div data-w-tab="Tab 1" class="tab-pane-financing w-tab-pane w--tab-active">
          <div class="scholarship-section">
            {% for block in tab1 %}
              <div class="content-wrapper">
                <div class="header-wrapper">
                  <h5 class="larger-h5">{{ block.settings.tab_header }}</h5>
                </div>
                <div class="scholarships-wrapper-all">
                  <div class="scholarship-ind">
                    <div class="schol-information-flex-row">
                      <div class="scholarship-main-flex-col">
                        <h4 class="bold-text blue-text">{{ block.settings.header }}</h4>
                        {% if block.settings.link != blank %}
                          <a href="{{block.settings.link}}" class="scholarship-link w-inline-block">
                            <div>Job board »</div>
                          </a>
                        {% endif %}
                      </div>
                      <div class="schol-inner-info-flex">
                        <div class="schol-flex-col col-12">
                          <div class="light-text">{{ block.settings.description }}</div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            {% endfor %}
          </div>
        </div>
      {% endif %}
