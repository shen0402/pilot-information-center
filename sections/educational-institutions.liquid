        <h3 class="white-text margin-b">{{ section.settings.tab3_header }}</h3>
      </div>
    {% endif %}
    {% if section.settings.tab4_header != blank %}
      <div class="types-financing-grid-inner">
        <div class="line-wrapper margin-b"><img
            src="//cdn.shopify.com/s/files/1/0714/2922/6772/files/line-element.svg?v=1674766851"
            loading="lazy"
            alt=""></div>
        <h3 class="white-text margin-b">{{ section.settings.tab4_header }}</h3>
      </div>
    {% endif %}
  </div>
  <div
    id="w-node-_92474ba0-2b29-f9cc-d16b-3df73a66a391-c4b47619"
    data-current="Tab 1"
    data-easing="ease"
    data-duration-in="300"
    data-duration-out="100"
    class="w-tabs">
    <div class="tabs-financing-grid w-tab-menu">
      {% if section.settings.tab1_header != blank %}
        <a data-w-tab="Tab 1" class="btn-wrapper-types w-inline-block w-tab-link w--current">
          <div>View »</div>
        </a>
      {% endif %}
      {% if section.settings.tab2_header != blank %}
        <a data-w-tab="Tab 2" class="btn-wrapper-types w-inline-block w-tab-link">
          <div>View »</div>
        </a>
      {% endif %}
      {% if section.settings.tab3_header != blank %}
