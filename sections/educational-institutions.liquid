<div class="types-section-financing training-tabs">
  <div class="types-financing-backgrndimg"></div>
  <div class="header-wrapper">
    <div class="tight-content-wrapper tabs-header">
      <h4>{{ section.settings.subtitle }}</h4>
      <h2>{{ section.settings.title }}</h2>
      <div class="top-inner-content margin-b">
        {{ section.settings.description }}
      </div>
    </div>
  </div>
  <div class="w-layout-grid types-financing-grid">
    {% if section.settings.tab1_header != blank %}
      <div class="types-financing-grid-inner">
        <div class="line-wrapper margin-b"><img
            src="//cdn.shopify.com/s/files/1/0714/2922/6772/files/line-element.svg?v=1674766851"
            loading="lazy"
            alt=""></div>
        <h3 class="white-text margin-b">{{ section.settings.tab1_header }}</h3>
      </div>
    {% endif %}
    {% if section.settings.tab2_header != blank %}
      <div class="types-financing-grid-inner">
        <div class="line-wrapper margin-b"><img
            src="//cdn.shopify.com/s/files/1/0714/2922/6772/files/line-element.svg?v=1674766851"
            loading="lazy"
            alt=""></div>
        <h3 class="white-text margin-b">{{ section.settings.tab2_header }}</h3>
      </div>
    {% endif %}
    {% if section.settings.tab3_header != blank %}
      <div class="types-financing-grid-inner">
